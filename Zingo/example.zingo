FUN update_points_state(points)
	VAR finalpoints = points + 1
    VAR multiplier = 5.2
	RETURN finalpoints + multiplier
END

FUN get_position_values(diff)
    VAR positions = []

    FOR i = 0 TO 5 THEN
		APPEND(positions, i + diff)
	END

	RETURN positions
END

FUN r(num)
	VAR result = num + 1
	RETURN result
END

